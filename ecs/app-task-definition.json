[
 { 
    "name": "myapp",
    "image": "450799241906.dkr.ecr.us-east-1.amazonaws.com/webapp:v1.0",
    "memory": 300,
    "portMappings": [
        {
            "containerPort": 8000
        }
    ],
    "essential": true,
    "environment": [
        {
            "name": "BUILD_ENV",
            "value": "dev"
        }
    ],
    "logConfiguration": { 
        "logDriver": "awslogs",
        "options": { 
           "awslogs-group" : "/ecs/fargate-task-definition",
           "awslogs-region": "us-east-1",
           "awslogs-stream-prefix": "ecs"
        }
    }
 }
]
